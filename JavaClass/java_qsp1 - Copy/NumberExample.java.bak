import java.util.Scanner;
class NumberExample 
{
	public static void main(String[] args) 
	{
       System.out.println("Enter a string:");
	   Scanner sc = new Scanner(System.in);

	   int num =sc. nextInt();
	   int dup=num;
	   int sum =0;
	  
	  while(num>0)
		{
		  int rem = num%10;
		  if(rem%2==0);
		  sum+=rem;
		  num/=10;
	}
	System.out.println(sum);
	System.out.println(dup);
		
		 
}
}