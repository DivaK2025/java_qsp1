class Pattern4 {
    public static void main(String[] args) {
        int n = 5;
        int i, j, k;
        
        for (i = 0; i < n; i++) 
			{ 
            for (j = 0; j < n; j++) 
				{ 
                System.out.print(" ");
            }
            for (k = 0; k <= n - i - 1; k++) 
				{ 
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
