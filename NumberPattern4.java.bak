class NumberPattern4{
	public static void main(String[]args){
		int n =5;
		int dif1 = 9, dif2=1;

		for(int i=1; i<=n; i++){
			int b=i;
			for(int j=1; j<n; j++){
				System.out.print(b+" ");
				if(j%2!=0)
					b+=dif1;
				else
					b+=dif2;
			}
			dif1-=2;
			dif2+=2;

			System.out.println();
		}
	}
}